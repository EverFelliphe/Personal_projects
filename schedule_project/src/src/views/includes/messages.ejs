<%if(errors.length > 0) { %>
  <div class="row">
      <div class="col my-3">
        <div class="alert alert-danger">
          <% errors.forEach(value => {%>
            <%= value %><br>
          <%});%>
        </div>
      </div>
  </div>
<% } %>

<%if(success.length > 0) { %>
  <div class="row">
      <div class="col my-3">
        <div class="alert alert-success">
          <% success.forEach(value => {%>
            <%= value %><br>
          <%});%>
        </div>
      </div>
  </div>
<% } %>
